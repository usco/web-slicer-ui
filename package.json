  "main": "src/index.js",
  "scripts": {
    "test": "ava './src/utils/**/*.test.js' --require babel-register --verbose",
    "build": "browserify src/index.js -g [envify --NODE_ENV 'production'] -o dist/index.js -t browserify-css  -t [babelify browserify minifyify]",
